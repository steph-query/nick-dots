{
  "name": "sr-salesforce-proxy",
  "version": "1.0.0",
  "description": "REST API Proxy to Salesforce, used for various BFF salesforce integration requirements",
  "main": "app.local.js",
  "author": "Chomp Labs",
  "license": "UNLICENSED ",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development node app.local.js",
    "start": "NODE_ENV=production node app.local.js",
    "offline": "npm run build && serverless offline start",
    "build": "next build src",
    "export": "npm run build && next export -o public",
    "package": "serverless package",
    "deploy": "npm run build && npm run package && serverless deploy --verbose",
    "test": "jest --watch",
    "coverage": "jest --coverage --collectCoverageFrom=src/**/*.js",
    "lint": "eslint ./src"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "express": "^4.16.3",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.10",
    "next": "^7.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "serverless-http": "^1.5.5",
    "styled-components": "^3.4.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.8.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.1",
    "eslint-config-jest-enzyme": "^6.1.2",
    "eslint-config-kentcdodds": "14.0.2",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.1.0",
    "jest-styled-components": "^6.2.1",
    "nodemon": "^1.17.5",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "^16.5.2",
    "serverless": "^1.28.0",
    "serverless-offline": "^3.25.4",
    "supertest": "^3.1.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testEnvironment": "node",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
